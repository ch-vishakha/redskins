{
    "name": "narendravaghela/cakephp-mailgun",
    "description": "MailgunEmail plugin for CakePHP",
    "license": "MIT",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "mailgun", "email"],
    "homepage": "https://github.com/narendravaghela/cakephp-mailgun",
    "authors": [
        {
            "name": "Narendra Vaghela",
            "homepage": "http://www.narendravaghela.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-mbstring": "*",
        "ext-json": "*",
        "cakephp/cakephp": "^3.4.1",
        "mailgun/mailgun-php": "2.1.*",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MailgunEmail\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MailgunEmail\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "Mailgun\\Tests\\": "./vendor/mailgun/mailgun-php/tests"
        }
    }
}
